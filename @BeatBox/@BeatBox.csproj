    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>_BeatBox</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="wwwroot\Image\category\" />
    <Folder Include="wwwroot\Image\song\" />
    <Folder Include="wwwroot\Songs\" />
